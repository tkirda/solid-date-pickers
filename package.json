{
  "name": "solid-date-pickers",
  "version": "0.2.0",
  "description": "SolidJS date picker components built with @suid/material",
  "keywords": [
    "solid",
    "date",
    "picker"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tkirda/solid-date-pickers",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "solid": "./dist/source/index.js",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "build:docs": "tsc && vite build --outDir ./docs",
    "type-check": "tsc",
    "test": "jest --verbose --runInBand",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkirda/solid-date-pickers.git"
  },
  "peerDependencies": {
    "@suid/icons-material": "^0.6.11",
    "@suid/material": "^0.15.1",
    "solid-js": ">=1.6.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "@suid/icons-material": "^0.6.11",
    "@suid/material": "^0.15.1",
    "@types/jest": "^29.5.11",
    "@types/prismjs": "^1.26.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.8.0",
    "rollup-preset-solid": "^2.0.1",
    "solid-js": "^1.8.7",
    "storybook": "^7.6.4",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-solid": "^2.8.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
